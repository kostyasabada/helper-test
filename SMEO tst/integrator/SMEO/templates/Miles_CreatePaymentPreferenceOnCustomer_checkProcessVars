<#assign paymentPreferenceId = "" />
<#if body.bpmnError?? && body.bpmnError?has_content>
{
    "objectToReturn": <@printObjectToJson source=body />
}
<#else>
    <#if body.processVariables.paymentPreferenceId?? && body.processVariables.paymentPreferenceId?has_content>
        <#assign paymentPreferenceId = body.processVariables.paymentPreferenceId />
    <#else>
        <#assign paymentPreferenceId = body.paymentPreferenceIdGet />
    </#if>
    {
        "objectToReturn": <@printObjectToJson source=body />,
        "processVariables": {
            "paymentPreferenceId": "${paymentPreferenceId}"
        },
        "saveToDB": [
            {
                "variableName": "paymentPreferenceId",
                "type": "STRING",
                "value": "${paymentPreferenceId}"
            }
        ]
    }
</#if>
