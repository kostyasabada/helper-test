    <#assign firstName = ""/>
    <#assign lastName = ""/>
    <#assign email = ""/>
<#list body.values as value>
        <#if value.name == 'givenName'>
         <#assign firstName = value.values[0]/>
         <#elseif value.name == 'sn'>
         <#assign lastName = value.values[0]/>
         <#elseif value.name == 'mail'>
         <#assign email = value.values[0]/>
      </#if>
</#list>
  {
    "firstName": "${firstName}",
    "lastName": "${lastName}",
    "email": "${email}"
  }
